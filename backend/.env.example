# Core Application Settings
JWT_SECRET=your_secure_jwt_secret_here
MONGODB_URI=mongodb://username:password@host:port/database
PORT=5000
NODE_ENV=development
FRONTEND_URL=http://localhost:3000

# OpenAI Configuration (Primary LLM Provider)
OPENAI_API_KEYS=key1,key2,key3
OPENAI_ORGANIZATION_ID=your_org_id_here
OPENAI_DEFAULT_MODEL=gpt-4o
OPENAI_REQUEST_TIMEOUT=60000

# Anthropic Configuration (Alternative LLM Provider)
ANTHROPIC_API_KEYS=key1,key2,key3
ANTHROPIC_DEFAULT_MODEL=claude-3-opus-20240229
ANTHROPIC_REQUEST_TIMEOUT=60000

# Azure OpenAI Configuration (Optional Enterprise LLM Provider)
AZURE_OPENAI_API_KEYS=key1,key2,key3
AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com
AZURE_OPENAI_DEFAULT_DEPLOYMENT=your_deployment_name
AZURE_OPENAI_REQUEST_TIMEOUT=60000

# Global AI Settings
AI_DEFAULT_PROVIDER=openai
AI_CACHE_ENABLED=true
AI_CACHE_TTL_SECONDS=3600
AI_MAX_RETRIES=3
AI_RETRY_DELAY_MS=1000
AI_RATE_LIMIT_PER_MINUTE=60
AI_COST_TRACKING_ENABLED=true

# GitHub Integration (for solution repository analysis)
GITHUB_API_TOKENS=token1,token2,token3